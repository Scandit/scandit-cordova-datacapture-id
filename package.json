{
  "name": "scandit-cordova-datacapture-id",
  "version": "8.1.0",
  "description": "Scandit Data Capture SDK for Cordova",
  "license": "Apache-2.0",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/dts/index.d.ts",
  "keywords": [
    "ecosystem:cordova",
    "cordova-ios",
    "cordova-android"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "test": "npm run lint",
    "lint": "eslint ./www/ts",
    "coverage": "jest --coverage --maxWorkers=12",
    "clear-jest-cache": "jest --clearCache || true",
    "clean": "npm run clear-jest-cache && rimraf dist coverage",
    "clean-all": "npm run clean && rimraf node_modules"
  },
  "dependencies": {
    "scandit-cordova-datacapture-barcode": "8.1.0",
    "scandit-cordova-datacapture-core": "8.1.0",
    "scandit-cordova-datacapture-parser": "8.1.0",
    "scandit-datacapture-frameworks-barcode": "8.1.0",
    "scandit-datacapture-frameworks-core": "8.1.0",
    "scandit-datacapture-frameworks-id": "8.1.0",
    "scandit-datacapture-frameworks-parser": "8.1.0"
  },
  "cordova": {
    "id": "scandit-cordova-datacapture-id",
    "platforms": [
      "ios",
      "android"
    ]
  }
}
